[user]
  name = apidcloud
  email = luisapidcloud@hotmail.com
[alias]
  cdiff = "!git diff $1 $1^"

[core]
  pager = delta --theme="night-owlish"
	excludesfile = /Users/luis/.gitignore_global
[interactive]
    diffFilter = delta --color-only
[color]
	ui = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 11
	frag = magenta bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
